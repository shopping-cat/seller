{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@apollo/client": "^3.3.11",
    "antd": "^4.14.0",
    "apollo-upload-client": "^14.1.3",
    "firebase": "^8.3.0",
    "next": "10.0.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^14.1.0",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "typescript": "^4.2.3"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}